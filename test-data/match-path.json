{
  "request": {
    "urlPath": "/everything",
    "method": "ANY",
    "headers": {
      "Accept": {
        "contains": "xml"
      }
    },
    "queryParameters": {
      "search_term": {
        "equalTo": "WireMock"
      }
    },
    "cookies": {
      "session": {
        "matches": ".*12345.*"
      }
    },
    "bodyPatterns": [
      {
        "equalToXml": "<search-results />"
      },
      {
        "matchesXPath": "//search-results"
      }
    ],
    "multipartPatterns": [
      {
        "matchingType": "ANY",
        "headers": {
          "Content-Disposition": {
            "contains": "name=\"info\""
          },
          "Content-Type": {
            "contains": "charset"
          }
        },
        "bodyPatterns": [
          {
            "equalToJson": "{}"
          }
        ]
      }
    ],
    "basicAuthCredentials": {
      "username": "jeff@example.com",
      "password": "jeffteenjefftyjeff"
    }
  },
  "response": {
    "status": 200
  }
}
